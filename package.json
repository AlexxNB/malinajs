{
  "name": "malinajs",
  "scripts": {
    "build_app": "node ./src/main.js && babel ./bin/output.js -o ./bin/output.js && uglifyjs ./bin/output.js -o ./bin/output.min.js -m",
    "build_app_es5": "babel ./bin/output.js -o ./bin/output.es5.js --presets babel-preset-es2015 && uglifyjs ./bin/output.es5.js -o ./bin/output.es5.min.js -m",
    "build_compiler": "rollup -c",
    "web": "http-server ./bin"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "acorn": "^7.3.1",
    "astring": "^1.4.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "http-server": "^0.12.3",
    "rollup": "^2.17.1",
    "uglify-es": "^3.3.9"
  }
}
